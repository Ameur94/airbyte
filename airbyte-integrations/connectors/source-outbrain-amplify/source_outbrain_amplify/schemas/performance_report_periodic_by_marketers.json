{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "metadata": {
      "description": "Metadata containing date range information for the report",
      "type": "object",
      "properties": {
        "id": {
          "description": "Unique identifier for the metadata",
          "type": ["string", "null"]
        },
        "fromDate": {
          "description": "Start date of the report period",
          "type": ["string", "null"]
        },
        "toDate": {
          "description": "End date of the report period",
          "type": ["string", "null"]
        }
      }
    },
    "metrics": {
      "description": "Performance metrics for the marketer",
      "type": "object",
      "properties": {
        "impressions": {
          "description": "Number of ad impressions",
          "type": ["number", "null"]
        },
        "clicks": {
          "description": "Number of clicks",
          "type": ["number", "null"]
        },
        "totalConversions": {
          "description": "Total number of conversions",
          "type": ["number", "null"]
        },
        "conversions": {
          "description": "Number of conversions",
          "type": ["number", "null"]
        },
        "viewConversions": {
          "description": "Number of conversions from video views",
          "type": ["number", "null"]
        },
        "spend": {
          "description": "Total amount spent on advertising",
          "type": ["number", "null"]
        },
        "ecpc": {
          "description": "Effective cost per click",
          "type": ["number", "null"]
        },
        "ctr": {
          "description": "Click-through rate percentage",
          "type": ["number", "null"]
        },
        "conversionRate": {
          "description": "Percentage of conversions out of total clicks",
          "type": ["number", "null"]
        },
        "viewConversionRate": {
          "description": "Percentage of conversions out of total video views",
          "type": ["number", "null"]
        },
        "cpa": {
          "description": "Cost per acquisition",
          "type": ["number", "null"]
        },
        "totalCpa": {
          "description": "Total cost per acquisition",
          "type": ["number", "null"]
        },
        "totalSumValue": {
          "description": "Total sum value generated",
          "type": ["number", "null"]
        },
        "sumValue": {
          "description": "Total value generated by events",
          "type": ["number", "null"]
        },
        "viewSumValue": {
          "description": "Total value generated from video views",
          "type": ["number", "null"]
        },
        "totalAverageValue": {
          "description": "Total average value generated",
          "type": ["number", "null"]
        },
        "averageValue": {
          "description": "Average value per event",
          "type": ["number", "null"]
        },
        "viewAverageValue": {
          "description": "Average value per view event",
          "type": ["number", "null"]
        },
        "totalRoas": {
          "description": "Total return on ad spend",
          "type": ["number", "null"]
        },
        "roas": {
          "description": "Return on ad spend",
          "type": ["number", "null"]
        },
        "videoReachedFirstQ": {
          "description": "Number of times video ad reached first quartile",
          "type": ["integer", "null"]
        },
        "videoReachedSecondQ": {
          "description": "Number of times video ad reached second quartile",
          "type": ["integer", "null"]
        },
        "videoReachedThirdQ": {
          "description": "Number of times video ad reached third quartile",
          "type": ["integer", "null"]
        },
        "videoReachedCompletion": {
          "description": "Number of times video ad reached completion",
          "type": ["integer", "null"]
        },
        "videoViewDuration": {
          "description": "Total duration of video views",
          "type": ["integer", "null"]
        },
        "videoAvgViewDuration": {
          "description": "Average duration of video views",
          "type": ["integer", "null"]
        },
        "videoPlays": {
          "description": "Number of times the video ad was played",
          "type": ["integer", "null"]
        }
      }
    },
    "marketer_id": {
      "description": "The unique identifier for the marketer",
      "type": ["string", "null"]
    }
  }
}
