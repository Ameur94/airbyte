{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://example.com/object1667390919.json",
  "title": "Root",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier of the collection.",
      "$id": "#root/id",
      "title": "Id",
      "type": "string",
      "default": "",
      "pattern": "^.*$"
    },
    "media": {
      "description": "Contains media items related to the collection",
      "$id": "#root/media",
      "title": "Media",
      "type": "array",
      "default": [],
      "items": {
        "description": "Represents each individual media item in the collection",
        "$id": "#root/media/items",
        "title": "Items",
        "type": "object",
        "properties": {
          "type": {
            "description": "Type of the media item.",
            "$id": "#root/media/items/type",
            "title": "Type",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "id": {
            "description": "The unique identifier of the media item.",
            "$id": "#root/media/items/id",
            "title": "Id",
            "type": "integer",
            "default": 0
          },
          "width": {
            "description": "Width of the media item in pixels.",
            "$id": "#root/media/items/width",
            "title": "Width",
            "type": "integer",
            "default": 0
          },
          "height": {
            "description": "Height of the media item in pixels.",
            "$id": "#root/media/items/height",
            "title": "Height",
            "type": "integer",
            "default": 0
          },
          "url": {
            "description": "URL of the media item.",
            "$id": "#root/media/items/url",
            "title": "Url",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "photographer": {
            "description": "Name of the photographer of the media item.",
            "$id": "#root/media/items/photographer",
            "title": "Photographer",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "photographer_url": {
            "description": "URL to the photographer's profile.",
            "$id": "#root/media/items/photographer_url",
            "title": "Photographer_url",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "photographer_id": {
            "description": "Unique identifier of the photographer.",
            "$id": "#root/media/items/photographer_id",
            "title": "Photographer_id",
            "type": "integer",
            "default": 0
          },
          "avg_color": {
            "description": "Average color of the media item.",
            "$id": "#root/media/items/avg_color",
            "title": "Avg_color",
            "type": ["null", "string"],
            "default": "",
            "pattern": "^.*$"
          },
          "src": {
            "description": "The source URL of the media item",
            "$id": "#root/media/items/src",
            "title": "Src",
            "type": "object",
            "properties": {
              "original": {
                "description": "URL of the original version of the media item.",
                "$id": "#root/media/items/src/original",
                "title": "Original",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "large2x": {
                "description": "URL of the double-sized large version of the media item.",
                "$id": "#root/media/items/src/large2x",
                "title": "Large2x",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "large": {
                "description": "URL of the large version of the media item.",
                "$id": "#root/media/items/src/large",
                "title": "Large",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "medium": {
                "description": "URL of the medium-sized version of the media item.",
                "$id": "#root/media/items/src/medium",
                "title": "Medium",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "small": {
                "description": "URL of the small version of the media item.",
                "$id": "#root/media/items/src/small",
                "title": "Small",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "portrait": {
                "description": "URL of the portrait-oriented version of the media item.",
                "$id": "#root/media/items/src/portrait",
                "title": "Portrait",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "landscape": {
                "description": "URL of the landscape-oriented version of the media item.",
                "$id": "#root/media/items/src/landscape",
                "title": "Landscape",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "tiny": {
                "description": "URL of the tiny version of the media item.",
                "$id": "#root/media/items/src/tiny",
                "title": "Tiny",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              }
            }
          },
          "liked": {
            "description": "Flag indicating if the media item is liked.",
            "$id": "#root/media/items/liked",
            "title": "Liked",
            "type": "boolean",
            "default": true
          }
        }
      }
    },
    "page": {
      "description": "Current page number for pagination.",
      "$id": "#root/page",
      "title": "Page",
      "type": "integer",
      "default": 0
    },
    "per_page": {
      "description": "Number of items per page.",
      "$id": "#root/per_page",
      "title": "Per_page",
      "type": "integer",
      "default": 0
    },
    "total_results": {
      "description": "Total number of results in the collection.",
      "$id": "#root/total_results",
      "title": "Total_results",
      "type": "integer",
      "default": 0
    },
    "next_page": {
      "description": "Next page number for pagination.",
      "$id": "#root/next_page",
      "title": "Next_page",
      "type": "string",
      "default": "",
      "pattern": "^.*$"
    },
    "prev_page": {
      "description": "Previous page number for pagination.",
      "$id": "#root/prev_page",
      "title": "Prev_page",
      "type": "string",
      "default": "",
      "pattern": "^.*$"
    }
  }
}
