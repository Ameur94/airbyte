data:
  ab_internal:
    ql: 100
    sl: 100
  allowedHosts:
    hosts:
      - "{{ host }}"
  connectorBuildOptions:
    baseImage: {{ base_image }}
  connectorSubtype: api
  connectorType: source
  definitionId: {{ definition_id }}
  dockerImageTag: {{ version }}
  dockerRepository: airbyte/source-{{source_name}}
  documentationUrl: https://docs.airbyte.com/integrations/sources/{{source_name}}
  githubIssueLabel: source-{{source_name}}
  icon: {{source_name}}.svg
  license: MIT
  maxSecondsBetweenMessages: 60
  name: {{ source_name | capitalize }}
  remoteRegistries:
    pypi:
      enabled: false
  registries:
    cloud:
      enabled: true
    oss:
      enabled: true
  releaseStage: alpha
  supportLevel: community
  tags:
    - language:python
    - cdk:python
  connectorTestSuitesOptions:
    - suite: unitTests
    - suite: acceptanceTests
      testSecrets:
metadataSpecVersion: "1.0"
